            <fs18i>X</fs18i>
            <fs30ia>X</fs30ia>
            <fs30ib>H</fs30ib>
        </l>
        <w>
            <fs0id>X</fs0id>
            <fs0if>X</fs0if>
            <fs16i>O</fs16i>
