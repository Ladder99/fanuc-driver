            <fs30ib>X</fs30ib>
        </w>
        <pw>
            <d>O</d>
            <h>X</h>
            <a>X</a>
        </pw>

        <is>
            <m>
                <fs30isa>O</fs30isa>
                <fs30isb>O</fs30isb>
            </m>
            <t>
                <fs30isa>O</fs30isa>
                <fs30isb>O</fs30isb>
            </t>
            <w>
                <fs30isa>X</fs30isa>
                <fs30isb>X</fs30isb>
